# Local Development Environment Configuration
# Copy of .env.example for immediate development use

# Development API Configuration
# In development, we use the Vite proxy, so this is relative
VITE_API_BASE_URL=/api/v1

# Development Settings
VITE_NODE_ENV=development
VITE_DEBUG=true

# Feature Flags
VITE_ENABLE_EVOLUTION_CHAIN=true
VITE_ENABLE_RECOMMENDATIONS=true
VITE_ENABLE_TYPE_FILTERING=true

# API Configuration
VITE_API_TIMEOUT_MS=30000
VITE_RETRY_ATTEMPTS=3

# UI Configuration
VITE_DEBOUNCE_MS=300
VITE_DEFAULT_LIMIT=50
VITE_GRID_MIN_WIDTH=140

# Health Check Configuration
VITE_HEALTH_CHECK_INTERVAL=30000
VITE_HEALTH_CHECK_TIMEOUT=5000